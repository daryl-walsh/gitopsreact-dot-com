{
  "name": "gitopsreact",
  "version": "0.1.0",
  "description": "developing and deploying a react application using containers and a full CI/CD lifecycle",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && babel src -d dist --copy-files",
    "dev:serve": "npm run build && dotenv -e envs/.env node dist/index.js",
    "dev:test": "npm run test:unit && npm run test:integration && npm run test:e2e:prepare && yarn run test:e2e",
    "dev:watch": "nodemon -w src --exec yarn run serve",
    "fix": "eslint src --fix",
    "lint": "eslint src",
    "precommit": "npm run lint",
    "serve": "npm run build && dotenv -e envs/.env pm2 start dist/index.js",
    "test": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:serve": "dotenv -e envs/test.env -e envs/.env babel-node src/index.js",
    "test:coverage": "nyc --reporter=html --reporter=text npm run test",
    "test:unit": "mocha 'src/**/*.unit.test.js' --require @babel/register",
    "test:unit:coverage": "nyc --reporter=html --reporter=text yarn run test:unit",
    "test:integration": "dotenv -e envs/test.env -e envs/.env mocha -- 'src/**/*.integration.test.js' --require @babel/register",
    "test:integration:coverage": "nyc --reporter=html --reporter=text yarn run test:integration",
    "test:e2e": "dotenv -e envs/test.env -e envs/.env ./scripts/e2e.test.sh",
    "test:e2e:prepare": "dotenv -e envs/test.env -e envs/.env ./scripts/e2e.prepare.sh",
    "test:e2e:coverage": "nyc --reporter=html --reporter=text npm run test:e2e"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint src --ext .js --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/darylwalsh/gitopsreact-dot-com.git"
  },
  "keywords": [
    "react",
    "jest",
    "mocha",
    "cucumber",
    "puppeteer",
    "docker",
    "kubernetes",
    "jenkins",
    "elastic",
    "search"
  ],
  "author": "Daryl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darylwalsh/gitopsreact-dot-com/issues"
  },
  "homepage": "https://github.com/darylwalsh/gitopsreact-dot-com#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.0.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "babel-eslint": "^10.0.1",
    "chance": "^1.0.18",
    "cucumber": "^5.1.0",
    "dotenv-cli": "^2.0.0",
    "eslint": "^5.16.0",
    "eslint-config-jest-enzyme": "^7.0.2",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jsonfile": "^5.0.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "mocha": "^5.2.0",
    "ndb": "^1.0.44",
    "nodemon": "^1.18.10",
    "nyc": "^13.0.1",
    "object-path": "^0.11.4",
    "pm2": "^3.1.2",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.1",
    "superagent": "^5.0.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "ajv": "^6.10.0",
    "body-parser": "^1.18.3",
    "elasticsearch": "^15.4.1",
    "express": "^4.16.4"
  }
}
