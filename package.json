{
  "name": "gitopsreact",
  "version": "0.1.0",
  "description": "developing and deploying a react application using containers and a full CI/CD lifecycle",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist/ && babel src -d dist",
    "fix": "eslint src --fix",
    "lint": "eslint src",
    "precommit": "npm run lint",
    "serve": "npm run build && dotenv -e envs/.env node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:e2e": "dotenv -e envs/test.env -e envs/.env ./node_modules/.bin/cucumber-js -- spec/cucumber/features --require-module @babel/register --require spec/cucumber/steps",
    "watch": "nodemon -w src --exec npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/darylwalsh/gitopsreact-dot-com.git"
  },
  "keywords": [
    "react",
    "jest",
    "mocha",
    "cucumber",
    "puppeteer",
    "docker",
    "kubernetes",
    "jenkins",
    "elastic",
    "search"
  ],
  "author": "Daryl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darylwalsh/gitopsreact-dot-com/issues"
  },
  "homepage": "https://github.com/darylwalsh/gitopsreact-dot-com#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.2",
    "babel-eslint": "^10.0.1",
    "cucumber": "^5.1.0",
    "dotenv-cli": "^2.0.0",
    "eslint": "^5.16.0",
    "eslint-config-jest-enzyme": "^7.0.2",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "ndb": "^1.0.44",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "superagent": "^5.0.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "body-parser": "^1.18.3",
    "elasticsearch": "^15.4.1",
    "express": "^4.16.4"
  }
}
